# MongoDB設定
MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/database?retryWrites=true&w=majority&appName=AppName
MONGODB_DB=your-database-name

# JWT設定
JWT_SECRET=your-very-long-random-jwt-secret-key-here

# API設定
API_KEYS_DATA={"keys":[]}
DEFAULT_API_KEY=your-default-api-key

# 管理者設定
ADMIN_USERNAME=admin
ADMIN_PASSWORD=your-secure-admin-password
ADMIN_EMAIL=admin@example.com

# GitHub Fine-grained Personal Access Token
# 以下のURLで生成: https://github.com/settings/personal-access-tokens/new
# 必要な権限: Contents (read/write), Metadata (read), Pull requests (read/write)
GITHUB_TOKEN=github_pat_your_token_here

# GitHub設定
GITHUB_OWNER=your-github-username
GITHUB_REPO=your-repository-name
GITHUB_API_URL=https://api.github.com
